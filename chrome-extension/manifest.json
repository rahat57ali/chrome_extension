{
  "manifest_version": 3,
  "name": "Auth Extension",
  "version": "1.0",
  "description": "Login with backend auth",
  "permissions": ["storage", "tabs"],
  "host_permissions": ["http://localhost:5000/"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon.png"
    }
  },
  "web_accessible_resources": [{
    "resources": ["oauth-success.html","superAdmin.html", "superAdmin.js"],
    "matches": ["<all_urls>"]
  }]
}
